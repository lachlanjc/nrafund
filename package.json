{
  "name": "nrafund",
  "version": "1.0.0",
  "description": "Let's take Congress back from the NRA.",
  "private": true,
  "scripts": {
    "build": "nwb build-react-app",
    "clean": "nwb clean-app",
    "css": "postcss node_modules/basscss-margin/index.css -u postcss-custom-properties -o src/css/basscss-margin.css && postcss node_modules/basscss-padding/index.css -u postcss-custom-properties -o src/css/basscss-padding.css",
    "deploy": "npm run build && cp CNAME dist/CNAME && gh-pages -d dist",
    "start": "nwb serve-react-app --port 4000",
    "scrape": "node parse/scraper.js",
    "parse": "node parse/parse.js",
    "data": "npm run scrape && npm run parse",
    "render": "static-react --no-doctype src/Render.js > public/rendered.html",
    "analysis": "static-react --no-doctype src/Analysis.js > public/analysis.html"
  },
  "dependencies": {
    "react": "^15.2.1",
    "react-dom": "^15.2.1"
  },
  "devDependencies": {
    "axios": "^0.13.1",
    "basscss-margin": "^1.0.7",
    "basscss-padding": "^1.1.3",
    "comma-number": "^1.1.0",
    "gh-pages": "^0.11.0",
    "html-loader": "^0.4.3",
    "lodash": "^4.13.1",
    "moment": "^2.14.1",
    "nwb": "0.11.x",
    "postcss-cli": "^2.5.2",
    "postcss-custom-properties": "^5.0.1",
    "react-geomicons": "^2.0.5",
    "rebass": "^0.3.0",
    "reflexbox": "^2.1.0",
    "scrape-it": "^3.1.1",
    "static-react": "^3.2.0",
    "unformat-usd": "^0.1.1",
    "velocity-react": "^1.1.5"
  },
  "author": "Lachlan Campbell <lachlan.campbell@icloud.com>",
  "license": "MIT",
  "repository": ""
}
